 public static void threes(int dice1, int dice2, int dice3, int dice4, int dice5)
    {
        int a=0;

        if(dice1 == 3){
        a += 3;
        }

        if(dice2 == 3) {
         a += 3;
        }

        if(dice3 == 3){
        a += 3;
        }

        if(dice4 == 3){
        a += 3;
        }

        if(dice5 == 3){
        a += 3;
        }

        System.out.println("Threes         "+a);

    } 
